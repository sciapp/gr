        <xs:unique name="_bboxidisunique">
            <xs:selector xpath=".//*"/>
            <xs:field xpath="@_bbox_id"/>
        </xs:unique>
        <xs:unique name="figureidisunique">
            <xs:selector xpath="figure"/>
            <xs:field xpath="@_figure_id"/>
        </xs:unique>
    </xs:element>

